{
    "root": true,
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "plugin:angular/johnpapa",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "angular/file-name": 0
    }
}
